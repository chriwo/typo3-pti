{
  "name": "pti/pti",
  "type": "typo3-cms-extension",
  "description": "Prototype Integration",
  "authors": [
    {
      "name": "Hannes Lau",
      "email": "office@hanneslau.de"
    }
  ],
  "require": {
    "php" : "^7.4 || ^8.0",
    "typo3/cms-core": "^10.4.0 || ^11.5.0"
  },
  "autoload": {
    "psr-4": {
      "PrototypeIntegration\\PrototypeIntegration\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "pti"
    }
  },
  "replace": {
    "typo3-ter/pti": "self.version"
  }
}
